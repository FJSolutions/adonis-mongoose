{
  "name": "@fjsolutions/adonis-mongoose",
  "version": "2.0.0",
  "description": "A Mongoose provider for AdonisJS 5",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run clean && tsc && cp -r ./templates ./build/ && cp -r ./src/commands/templates ./build/commands",
    "build:watch": "npm run build -- --watch",
    "clean": "cd build && rm -fr * && cd .."
  },
  "author": "Francis Judge",
  "license": "MIT",
  "dependencies": {
    "@adonisjs/ace": "^6.9.4",
    "@adonisjs/core": "^5.0.0-preview-rc-1.12",
    "@adonisjs/fold": "^6.4.0",
    "@adonisjs/validator": "^7.4.2",
    "@fjsolutions/mongodb-uri": "^1.1.1",
    "mongoose": "^5.10.2",
    "voca": "^1.4.0"
  },
  "devDependencies": {
    "@types/mongoose": "^5.7.36",
    "@types/node": "^14.6.2",
    "@types/voca": "^1.4.0",
    "node-ts": "^5.0.8",
    "typescript": "^3.9.7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FJSolutions/adonis-mongoose.git"
  },
  "bugs": {
    "url": "https://github.com/FJSolutions/adonis-mongoose/issues"
  },
  "homepage": "https://github.com/FJSolutions/adonis-mongoose#readme",
  "types": "./build/index.d.ts",
  "adonisjs": {
    "templates": {
      "config": [
        "database.ts.mustache"
      ],
      "contracts": [
        "mongoose.ts.mustache",
        "validator.ts.mustache"
      ]
    },
    "env": {
      "DB_HOST": "localhost",
      "DB_PORT": 27017,
      "DB_USER": "",
      "DB_PASSWORD": "",
      "DB_DATABASE": "adonis",
      "DB_AUTH_SOURCE": "admin",
      "DB_SSL": false
    },
    "providers": [
      "@fjsolutions/adonis-mongoose/build/providers/MongooseProvider"
    ],
    "commands": [
      "@fjsolutions/adonis-mongoose/build/commands"
    ]
  }
}
